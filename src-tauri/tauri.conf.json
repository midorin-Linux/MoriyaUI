{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "moriyaui",
  "version": "0.1.0",
  "identifier": "com.wayokan.moriyaui",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "MoriyaUI",
        "width": 1280,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/favicon.ico"
    ]
  }
}
